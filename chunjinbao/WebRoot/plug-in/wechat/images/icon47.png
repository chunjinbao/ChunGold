<!--浮动导航JS-->
<script type="text/javascript">
var showBar = getCookie("ISBAR")=="1"?false:true;
var isindex = $(".isindex");
if(isindex.length){
	showBar=true;
}
if(showBar){
	foot_fk_box();
}
function foot_fk_box(){
	var foot_fk_box = document.getElementById("u-rbar");
	var foot_fk_body = document.getElementById("body1120");

	$("#u-rbar").width(105);//重新设置u-rbar的宽度

	if(window.innerWidth<1416){
	  foot_fk_box.style.right="10px";
	  foot_fk_box.style.left="auto";
	}
	else{
	  var sw = window.innerWidth ? window.innerWidth : document.documentElement.offsetWidth;
	  var topleft = foot_fk_body ? (sw-1120)/2+1225 : (sw-1000)/2+1105
	  foot_fk_box.style.left=topleft+"px";
	  foot_fk_box.style.right="auto";
	}
	if($(window).width()<=640){
		foot_fk_box.style.display="none";
	}else{
		foot_fk_box.style.display="block";
	}
	
}
$(".bar-close").click(function(){
	$("#u-rbar").hide();
	setCookie("ISBAR",1,0);
	showBar=false;
})
/*页面重新计算底部u-rbar的位置*/
$(window).resize(function(){
	if(showBar){
		foot_fk_box();
	}
})
$("#backup").hide();//go back to top
$(function () {
	$(window).scroll(function(){
		if ($(window).scrollTop()>180){$("#backup").fadeIn(500);}
		else {$("#backup").fadeOut(500);}
	});
	$("#backup").click(function(){
		$('body,html').animate({scrollTop:0},500);
			return false;
	});
});


</script>
<!--end浮动导航JS-->
     	

